{
  "name": "sample-frontend",
  "type": "frontend",
  "repo_path_or_url": "./apps/sample_frontend",
  "build_command": "echo 'No build required'",
  "start_command": "python -m http.server 8080",
  "health_endpoint": "/index.html",
  "environments": ["dev", "stage", "prod"],
  "port": 8080,
  "resources": {
    "cpu": "0.1",
    "memory": "128Mi"
  },
  "scaling": {
    "min_replicas": 1,
    "max_replicas": 3,
    "target_cpu_percent": 80
  }
}
# Development Environment Configuration
ENVIRONMENT=dev
DEBUG=true
LOG_LEVEL=DEBUG

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cicd_dev
DB_USER=dev_user

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Deployment
DEPLOYMENT_TIMEOUT=30
RETRY_COUNT=3
HEALTH_CHECK_INTERVAL=10

# Thresholds (relaxed for dev)
LATENCY_THRESHOLD_MS=20000
LOW_SCORE_THRESHOLD=30
HIGH_HEART_RATE=130
LOW_OXYGEN_LEVEL=90

# Monitoring
ENABLE_METRICS=true
DASHBOARD_PORT=8501
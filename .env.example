# Multi-Agent CI/CD System Configuration
# Copy this file to .env and update with your values

# Dataset Configuration
DATASET_PATH=dataset/student.csv
PLANNER_TYPE=rl

# Logging
LOG_LEVEL=INFO

# Dashboard Configuration
DASHBOARD_PORT=8501
DASHBOARD_AUTO_REFRESH=true

# MCP Endpoints Configuration
MCP_PORT=8080
MCP_HOST=0.0.0.0

# Detection Thresholds
LATENCY_THRESHOLD_MS=24000
LOW_SCORE_AVG=40
HIGH_HEART_RATE=120
LOW_OXYGEN_LEVEL=95

# Directory Paths
LOGS_DIR=./logs
DATA_DIR=./dataset
INSIGHTFLOW_DIR=./insightflow

# Redis Configuration (optional - uses in-memory if not set)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=<YOUR_PASSWORD_HERE>

# AWS Configuration (optional - for cloud deployment)
# AWS_ACCESS_KEY_ID=<YOUR_KEY_HERE>
# AWS_SECRET_ACCESS_KEY=<YOUR_SECRET_HERE>
# AWS_REGION=us-east-1

# API Keys (optional - for external integrations)
# API_KEY=<YOUR_API_KEY_HERE>
# API_SECRET=<YOUR_API_SECRET_HERE>
